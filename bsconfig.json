{
  "name": "rescript-project-template",
  "version": "0.0.1",
  "sources": {
    "dir" : "src",
    "subdirs" : true
  },
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "bsc-flags": ["-bs-super-errors", "-bs-no-version-header"],
  "suffix": ".bs.js",
  "bs-dependencies": ["@rescript/react", 
    "bs-fetch", 
    "@glennsl/bs-json", 
    "bs-css",
    "bs-css-emotion",
    "@heliaxdev/rescript-react-feather",
    "rescript-throttle-react"
  ],
  "warnings": {
    "error" : "+101"
  },
  "reason": { "react-jsx": 3 },
  "namespace": true
  
}
